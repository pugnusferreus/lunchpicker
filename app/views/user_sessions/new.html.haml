%h1 Login

- form_for @user_session, :url => user_session_path do |f|
  = f.error_messages

  %p
    = f.label :login
    %br
    = f.text_field :login

  %p
    = f.label :password
    %br
    = f.password_field :password

  %p
    = f.check_box :remember_me
    = f.label :remember_me

  = f.submit "Login"
